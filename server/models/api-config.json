{
  "name": "ApiConfig",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "index": true,
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "index": true     
    },
    "public": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "description": {
      "type": "string"
    },
    "entry": {
      "type": "string",
      "required": true,
      "default": "/"
    },
    "methods": {
      "type": [
        "string"
      ],
      "required": true,
      "default": "[\"GET\"]"
    },
    "errors": {
      "type": [
        "object"
      ]
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "plugins": {
      "type": "object"
    },
    "dependencies": {
      "type": [
        "string"
      ]
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}