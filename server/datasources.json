{
  "db": {
    "host": "${redis_host}",
    "port": "6379",
    "name": "db",
    "connector": "redis"
  },
  "auth": {
    "name": "auth",
    "connector": "memory",
    "file": "auth.json"
  },
  "storage": {
    "name": "storage",
    "connector": "loopback-component-storage",
    "provider": "filesystem",
    "root": "./storage"
  }
}